{
  "name": "bb-browser",
  "version": "1.0.0",
  "description": "Chrome-based browser automation for authenticated web scraping with proxy support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "bb-browser": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "tsx src/cli/index.ts",
    "serve": "tsx src/cli/index.ts serve",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "browser",
    "automation",
    "scraping",
    "playwright",
    "proxy"
  ],
  "author": "WarDekar",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "playwright": "^1.57.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
